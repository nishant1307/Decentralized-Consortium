    }
}



contract MinterRole is Ownable {
    using Roles for Roles.Role;

    event MinterAdded(address indexed account);
    event MinterRemoved(address indexed account);

    Roles.Role private _minters;

    constructor () internal {
        // _addMinter(msg.sender,"");
    }

    modifier onlyMinter(string memory projectName) {
        require(isMinter(msg.sender), "MinterRole: caller does not have the Minter role");
        _;
    }
    modifier onlyMinterWithProject(string memory projectName) {
        require(isMinterWithProject(msg.sender,projectName), "MinterRole: caller does not have the Minter role");
        _;
    }

    function isMinter(address account) public view returns (bool) {
        return _minters.has(account);
    }
    function isMinterWithProject(address account, string memory projectName) public view returns(bool){
        return _minters.checkProject(account,projectName);
    }

    function addMinter(address account, string memory projectName) public onlyOwner {
        _addMinter(account, projectName);
    }

    function renounceMinter(string memory projectName) public {
        _removeMinter(msg.sender,projectName);
    }

    function _addMinter(address account, string memory projectName) internal {
        _minters.add(account,projectName);
        emit MinterAdded(account);
    }

    function _removeMinter(address account, string memory projectName) internal {
        _minters.remove(account, projectName);
        emit MinterRemoved(account);
    }
}


