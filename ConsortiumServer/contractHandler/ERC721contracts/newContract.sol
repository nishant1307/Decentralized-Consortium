      uint toDelete = projectRegistry[projectName].index;
        delete projectRegistry[projectName];
         Project memory lastIndex = projects[projects.length-1];
         projectRegistry[lastIndex.name].index = toDelete;
        lastIndex.index = toDelete;
        projects[toDelete] = lastIndex;
        projects.length--;
